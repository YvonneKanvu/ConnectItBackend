generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Prestataire{
  Id Int @unique @default(autoincrement())
  Nom String
  PreNom String
  Adesse String
  Email String
  Telephone String
  Geolocalisation Geolocalisation @relation(fields: [GeolocalisationId],references: [Id])
  GeolocalisationId Int
  Secteur String
  NomDeL_entreprise String
  Fournir Fournir[]
  DemandeDeDevis DemandeDeDevis[]
  AvisEtRecommandention AvisEtRecommandention[]
  Message Message[]
  Intervention Intervention[]
  Appartenir Appartenir[]
}
model Geolocalisation{
  Id Int @unique @default(autoincrement())
  Adresse String
  Laltitude String
  Longitude String
  Prestataire Prestataire[]
}
model Utilisateur{
  Id Int @unique @default(autoincrement())
  Nom String
  PreNom String
  Email String
  Telephone String
  Fournir Fournir[]
  DemandeDeDevis DemandeDeDevis[]
  AvisEtRecommandention AvisEtRecommandention[]
  Message Message[]
  Intervention Intervention[]
}
model Fournir{
  Id Int @unique @default(autoincrement())
  Prestataire Prestataire? @relation(fields: [PrestataireId],references: [Id])
  PrestataireId Int?
  Utilisateur Utilisateur? @relation(fields: [UtilisateurId],references: [Id])
  UtilisateurId Int?
}
model DemandeDeDevis{
  Id Int @unique @default(autoincrement())
  TypeDeService String
  Statut String
  Date DateTime
  Prestataire Prestataire? @relation(fields: [PrestataireId],references: [Id])
  PrestataireId Int?
  Utilisateur Utilisateur? @relation(fields: [UtilisateurId],references: [Id])
  UtilisateurId Int?
}
model AvisEtRecommandention{
  Id Int @unique @default(autoincrement())
  Note String
  Commentaire String
  Date DateTime
  Prestataire Prestataire? @relation(fields: [PrestataireId],references: [Id])
  PrestataireId Int?
  Utilisateur Utilisateur? @relation(fields: [UtilisateurId],references: [Id])
  UtilisateurId Int?
}
model Message{
  Id Int @unique @default(autoincrement())
  Expediteur String
  Destinataire String
  Contenu String
  Statut String
  Date DateTime
  Prestataire Prestataire? @relation(fields: [PrestataireId],references: [Id])
  PrestataireId Int?
  Utilisateur Utilisateur? @relation(fields: [UtilisateurId],references: [Id])
  UtilisateurId Int?
}
model Intervention{
  Id Int @unique @default(autoincrement())
  Description String
  Statut String
  Date DateTime
  Prestataire Prestataire? @relation(fields: [PrestataireId],references: [Id])
  PrestataireId Int?
  Utilisateur Utilisateur? @relation(fields: [UtilisateurId],references: [Id])
  UtilisateurId Int?
}
model Secteur{
  Id Int @unique @default(autoincrement())
  Designation String 
  Appartenir Appartenir[]
}
model Appartenir{
  Id Int @unique @default(autoincrement())
  Prestataire Prestataire? @relation(fields: [PrestataireId],references: [Id])
  PrestataireId Int?
  Secteur Secteur? @relation(fields: [SecteurId],references: [Id])
  SecteurId Int?
}